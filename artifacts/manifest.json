{
    "manifest_version": 2,
    "name": "Karls Github Enhancer",
    "version": "4.2",
    "description": "Adds vital info to GitHub repos.",
    "content_scripts": [
      {
        "matches": ["*://*.github.com/*"],
        "js": [
          "node_modules/webextension-polyfill/dist/browser-polyfill.min.js",
          "github_vital_infos.js"
        ]
      }
    ],
    "web_accessible_resources": [
      "github_vital_infos_bg.wasm"
    ]
  }